{
    "manifest_version": 3,
    "name":             "Keep Twitter",
    "short_name":       "KeepTwitter",
    "version":          "2.0",
    "permissions":      ["favicon"],
    "content_scripts":  [
        {
        "matches": ["https://twitter.com/*", "https://x.com/*"],
        "js":      ["content_script.js"],
        "run_at": "document_end"
        }
    ],
    "devtools_page":    "popup.html",
    "homepage_url":     "https://github.com/Mikage32/KeepTwitter/wiki/"
}